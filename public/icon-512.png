<svg width="512" height="512" viewBox="0 0 512 512" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景グラデーション -->
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2563eb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7c3aed;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- 背景円 -->
  <rect width="512" height="512" rx="110" fill="url(#grad)"/>

  <!-- 花のアイコン（ブルーム） -->
  <g transform="translate(256, 256)">
    <!-- 中心の円 -->
    <circle cx="0" cy="0" r="40" fill="#fbbf24"/>

    <!-- 花びら（5枚） -->
    <!-- 上 -->
    <ellipse cx="0" cy="-90" rx="50" ry="70" fill="#ffffff" opacity="0.95"/>
    <!-- 右上 -->
    <ellipse cx="85" cy="-28" rx="50" ry="70" fill="#ffffff" opacity="0.95" transform="rotate(72 85 -28)"/>
    <!-- 右下 -->
    <ellipse cx="53" cy="72" rx="50" ry="70" fill="#ffffff" opacity="0.95" transform="rotate(144 53 72)"/>
    <!-- 左下 -->
    <ellipse cx="-53" cy="72" rx="50" ry="70" fill="#ffffff" opacity="0.95" transform="rotate(216 -53 72)"/>
    <!-- 左上 -->
    <ellipse cx="-85" cy="-28" rx="50" ry="70" fill="#ffffff" opacity="0.95" transform="rotate(288 -85 -28)"/>

    <!-- 接続線（コネクト） -->
    <line x1="-60" y1="-80" x2="60" y2="80" stroke="#60a5fa" stroke-width="8" opacity="0.7"/>
    <line x1="-60" y1="80" x2="60" y2="-80" stroke="#60a5fa" stroke-width="8" opacity="0.7"/>
  </g>
</svg>
